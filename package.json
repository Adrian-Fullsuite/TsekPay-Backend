{
  "name": "tsekpay-backend",
  "version": "1.0.0",
  "description": "TsekPay Backend API",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --inspect --env-file .env ./src/server.js",
    "migrate": "env $(cat .env) knex migrate:latest --knexfile ./src/database/knexfile.cjs",
    "rollback": "env $(cat .env) knex migrate:rollback --knexfile ./src/database/knexfile.cjs",
    "create-migration": "knex migrate:make $npm_config_name --migrations-directory src/database/migrations"
  },
  "author": "Fullsuite Engineering Team",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.0.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.9.1"
  }
}
